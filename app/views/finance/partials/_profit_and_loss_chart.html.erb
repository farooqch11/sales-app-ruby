  <header class="panel-heading font-bold">Sale Information</header>
  <div class="panel-body">
    <div class="row">
      <div class="col-sm-12">
        <div class="col-sm-6">
          <table class="highchart hidden table table-hover table-bordered" data-graph-container=".. .. .highchart-container3" data-graph-type="column">
            <!--<caption>Column + area example</caption>-->
            <thead>
            <tr>
              <th>Month</th>
              <th>Sales</th>
              <th data-graph-type="area">Profit</th>
            </tr>
            </thead>
            <tbody>

            <% @monthly_sales.sort.each do |month, sale| %>
                <% total_sale  = 0.0%>
                <% profit  = 0.00%>
                <% for s in sale %>
                    <% total_sale += s.total_payment %>
                    <% profit  += s.total_profit %>
                <% end %>

                <tr>
                  <td><%= month.strftime('%B %y') %></td>
                  <td><%= total_sale %></td>
                  <td><%= profit %></td>
                </tr>

            <% end %>
            </tbody>
          </table>
